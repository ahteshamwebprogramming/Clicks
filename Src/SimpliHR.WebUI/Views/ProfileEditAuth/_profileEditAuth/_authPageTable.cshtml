@model List<SimpliHR.Infrastructure.Models.ProfileEditAuth.ProfileEditAuthDTO>

<div class="table-responsive text-nowrap">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Field</th>
                <th>IsEditable</th>
                <th>Attachment Required</th>
            </tr>
        </thead>
        <tbody>
            @{
                if (Model != null)
                {
                    if (Model.Count > 0)
                    {
                        foreach (var item in Model)
                        {
                            <tr>
                                <td Profilefieldid="@item.ProfileFieldId">@item.ProfileFieldName</td>
                                <td name="IsEditable"><input type="checkbox" checked="@item.IsEditable"> </td>
                                <td name="AttachmentRequired"><input type="checkbox" checked="@item.AttachmentRequired"></td>
                            </tr>
                        }
                    }
                }
            }
        </tbody>
    </table>
</div>
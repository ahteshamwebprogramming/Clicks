@{
    //Layout = null;
    ViewData["Title"] = "Employee Dashboard";
}

<div class="container-xxl flex-grow-1 container-p-y">
    <h4 class="fw-bold py-3 mb-4"><span class="text-muted fw-light">Employee /</span> Dashboard</h4>
    <div class="card" id="List">
        <div class="card-datatable table-responsive text-nowrap">
            <table class="datatables-ajax table table-bordered List">
                <thead>
                    <tr>

                        <th>Full Name</th>
                        <th>Emp ID</th>
                        <th>DOJ</th>
                        <th>DOB</th>
                        <th>Department</th>
                        <th>Manager</th>
                        <th>Status</th>
                        <th>Action</th>
                    </tr>
                </thead>
                 <tbody>
                @if (Model.EmployeeMasterList != null)
                foreach (var item in Model.EmployeeMasterList)
                {
                <tr>
                <th></th>
                <th></th>
                <td>@item.CountryId</td>
                <td>@item.StateName</td>
                <th>@(item.IsActive == true ? "Active" : "Inactive")</th>
                <th> <a class="btn btn-primary" href="/Masters/GetCountry/@item.StateId">Edit</a>  <button class="btn btn-primary">Delete</button> </th>
                </tr>
                }
                </tbody>
            </table>
        </div>
    </div>


</div>


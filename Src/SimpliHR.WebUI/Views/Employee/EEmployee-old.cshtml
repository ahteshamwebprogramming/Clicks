                formData = "Language=" + language + "&Read=" + read + "&Write=" + write + "&Speak=" + speak + "&EmployeeLanguageDetailId=" + EmployeeLanguageDetailId + "&EmployeeId=" + jQuery("#EmployeeId").val() + "&FormName=" + jQuery("#FormName").val();

                                                <table class="table">
                                                    <thead>
                                                        <tr>
                                                            <th width="50"><input type="checkbox" ctrl="chk_parent_@(tabItem.Replace(" ",""))" class="form-check-input" /></th>
                                                            <th>Field</th>
                                                            <th>Current Value</th>
                                                            <th>New Value</th>
                                                            <th>View Attachment</th>
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>

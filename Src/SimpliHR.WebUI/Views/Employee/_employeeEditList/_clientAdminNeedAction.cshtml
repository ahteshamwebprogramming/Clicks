                                                                     <td class="text-center">
                                                                        @{
                                                                            if (item.IsApproved == null || item.IsApproved == 0)
                                                                            {
                                                                                <input type="checkbox" ctrl="chk_child_@(tabItem.Replace(" ",""))" class="form-check-input" />
                                                                            }
                                                                        }


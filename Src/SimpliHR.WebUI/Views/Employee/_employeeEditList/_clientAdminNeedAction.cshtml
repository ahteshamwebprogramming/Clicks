                                        <h2 class="accordion-header d-flex align-items-center">
                                            <button type="button"
                                                    class="accordion-button collapsed"
                                                    data-bs-toggle="collapse"
                                                    data-bs-target="#accordionWithIcon-@tabItem.Replace(" ","")"
                                                    aria-expanded="false">
                                                @tabItem
                                            </button>
                                        </h2>

                                                                            {
                                                                                <span style="color:orange">Cancelled</span>
                                                                            }
                                                                            else if (item.IsApproved == 2)
                                                                            {
                                                                                <span style="color:green">Approved</span>
                                                                            }
                                                                            else if (item.IsApproved == 3)
                                                                            {
                                                                                <span style="color:red">Rejected</span>
                                                                            }

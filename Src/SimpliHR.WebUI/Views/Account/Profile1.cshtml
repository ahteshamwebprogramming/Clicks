                        <div class="row">
                            <div class="col-12">
                                <div class="card mb-4">
                                    <div class="user-profile-header-banner">
                                        <img src="../../assets/img/pages/profile-banner.png" alt="Banner image" class="rounded-top" />
                                    </div>
                                    <div class="user-profile-header d-flex flex-column flex-sm-row text-sm-start text-center mb-4">
                                        <div class="flex-shrink-0 mt-n2 mx-sm-0 mx-auto">
                                            <img src="@((Model.Base64ProfileImage==null || Model.Base64ProfileImage=="data:image/png;base64,") ?  "../../assets/img/avatars/1.png" : Model.Base64ProfileImage)"
                                                 alt="user image"
                                                 class="d-block h-auto ms-0 ms-sm-4 rounded user-profile-img" />
                                        </div>
                                        <div class="flex-grow-1 mt-3 mt-sm-5">
                                            <div class="d-flex align-items-md-end align-items-sm-start align-items-center justify-content-md-between justify-content-start mx-4 flex-md-row flex-column gap-4">
                                                <div class="user-profile-info">
                                                    <h4>@(Model.EmployeeName == null ? "No Name Found" : Model.EmployeeName)</h4>
                                                    <ul class="list-inline mb-0 d-flex align-items-center flex-wrap justify-content-sm-start justify-content-center gap-2">
                                                        <li class="list-inline-item fw-semibold"><i class="bx bx-pen"></i> @(Model == null ? "No Jobtitle found" : Model.JobTitle == null ? "No Jobtitle found" : Model.JobTitle.JobTitle == null ? "" : Model.JobTitle.JobTitle) </li>
                                                        <li class="list-inline-item fw-semibold"><i class="bx bx-map"></i> @(Model == null ? "No location found" : Model.WorkLocation == null ? "No location found" : Model.WorkLocation.Location == null ? "" : Model.WorkLocation.Location)</li>
                                                        <li class="list-inline-item fw-semibold">
                                                            <i class="bx bx-calendar-alt"></i>  @(Model == null ? "Joined date not found" : Model.Doj == null ? "Joined date not found" : "Joined " + ((DateTime)Model.Doj).ToString("MMM-yyyy"))
                                                        </li>
                                                    </ul>
                                                </div>
                                                <a href="javascript:void(0)" class="btn btn-primary text-nowrap">
                                                    <i class="bx bx-user-check me-1"></i>Connected
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
